{
  "name": "swift-url-check",
  "version": "1.0.0",
  "description": "Fast and robust URL validator",
  "type": "module",
  "exports": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "benchmark": "node benchmark.js",
    "benchmark:compare": "node benchmark-comparison.js",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "url",
    "validator",
    "validation",
    "validate",
    "is-url",
    "check",
    "fast",
    "performance",
    "robust",
    "uri",
    "link",
    "http",
    "https"
  ],
  "author": "OpenSly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/opensly/swift-url-check.git"
  },
  "bugs": {
    "url": "https://github.com/opensly/swift-url-check/issues"
  },
  "homepage": "https://github.com/opensly/swift-url-check",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "is-url-superb": "^6.1.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "index.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/?(*.)+(spec|test).js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 100,
        "lines": 95,
        "statements": 95
      }
    },
    "transform": {}
  }
}
